@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ex: <http://example.com/ns#>. 

<#LoanApprovals>
 
rr:logicalTable [ rr:tableName "LOANAPPROVALS"];
		
	rr:subjectMap	[
		rr:template "http://foo.example.org/LoanApproval/{YEAR}";
		rr:termType rr:IRI;
		rr:class ex:LoanApprovals;
	];		
	
	rr:predicateObjectMap [
		rr:predicate foaf:Year;
		rr:objectMap  [ 
			rr:column "YEAR";
			rr:datatype xsd:integer;
			rr:termType rr:Literal;
		];	
	];

	rr:predicateObjectMap [
		rr:predicate ex:NewHouses;
		rr:objectMap  [ 
			rr:column "New Houses Number";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];

	rr:predicateObjectMap [
		rr:predicate ex:OtherHouses;
		rr:objectMap  [ 
			rr:column "Other Houses Number";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];

	rr:predicateObjectMap [
		rr:predicate ex:TotalHouses;
		rr:objectMap  [ 
			rr:column "Total Houses Number";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];

	
	
 .
